Host mercury
  HostName mercury.local
  User vagrant
  Port 22
  UserKnownHostsFile=/dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  IdentityFile ~/.ssh/nodes.key
  IdentitiesOnly yes

Host venus
  HostName venus.local
  User vagrant
  Port 22
  UserKnownHostsFile=/dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  IdentityFile ~/.ssh/nodes.key
  IdentitiesOnly yes